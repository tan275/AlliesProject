{"ast":null,"code":"import Navbar from '../components/Navbar.vue';\nimport friendList from '@/data/friends.json';\nexport default {\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      friends: friendList,\n      dialog: false\n    };\n  },\n  methods: {\n    searchInFriends(searchText) {\n      if (searchText) {\n        this.friends = friendList.filter(rec => rec.name.toLowerCase().includes(searchText));\n      } else {\n        this.friends = friendList;\n      }\n    }\n  }\n};","map":{"version":3,"names":["Navbar","friendList","components","data","friends","dialog","methods","searchInFriends","searchText","filter","rec","name","toLowerCase","includes"],"sources":["src/views/Friends.vue"],"sourcesContent":["<template>\r\n    <div class=\"body\">\r\n        <Navbar></Navbar>\r\n        <div class=\"container\">\r\n            <div class=\"profile-main\">\r\n                <h1 class=\"heading\">Allies </h1>\r\n                <v-text-field outlined label=\"Search\" append-icon=\"mdi-magnify\" @input=\"searchInFriends($event)\"\r\n                    class=\"heading\"></v-text-field>\r\n                <v-list-item v-for=\"(item, i) in friends\" :key=\"i\">\r\n                    <v-list-item-avatar tile>\r\n                        <v-img :src=\"item.img\"></v-img>\r\n                    </v-list-item-avatar>\r\n                    <v-list-item-content>\r\n                        <v-list-item-title>{{ item.name }}</v-list-item-title>\r\n                        <v-list-item-subtitle>{{ item.bio }}</v-list-item-subtitle>\r\n                        <v-list-item-subtitle>Status: {{ item.status }}</v-list-item-subtitle>\r\n                    </v-list-item-content>\r\n                    <v-list-item-icon>\r\n                        <div>\r\n                            <v-menu offset-y>\r\n                                <template v-slot:activator=\"{ on, attrs }\">\r\n                                    <v-btn v-bind=\"attrs\" v-on=\"on\" text>\r\n     