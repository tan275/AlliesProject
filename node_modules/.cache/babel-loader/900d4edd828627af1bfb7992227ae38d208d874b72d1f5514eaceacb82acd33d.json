Ratio: false,\n        scales: {\n          y: {\n            beginAtZero: true,\n            stepSize: 20\n          }\n        }\n      };\n      new Chart(this.$refs.monthlyChart, {\n        type: 'line',\n        data: monthlyData,\n        options: options\n      });\n    }\n  }\n};","map":{"version":3,"names":["Navbar","Chart","components","mounted","renderDailyChart","renderWeeklyChart","renderMonthlyChart","methods","dailyData","labels","datasets","label","borderColor","data","options","responsive","maintainAspectRatio","scales","y","beginAtZero","stepSize","$refs","dailyChart","type","weeklyData","weeklyChart","monthlyData","monthlyChart"],"sources":["src/views/Tracker.vue"],"sourcesContent":["<template>\r\n    <div class=\"body\">\r\n        <Navbar></Navbar>\r\n        <div class=\"container\">\r\n            <v-container>\r\n                <v-row>\r\n                    <v-col cols=\"12\">\r\n                        <h1 class=\"text-center\">Time Tracker</h1>\r\n                    </v-col>\r\n                </v-row>\r\n                <v-row>\r\n                    <v-col cols=\"12\" md=\"4\">\r\n                        <v-card>\r\n                            <v-card-title class=\"text-center\">Time Spent on Allies (Daily)</v-card-title>\r\n                            <v-card-text>\r\n                                <div class=\"chart-container\">\r\n                                    <canvas ref=\"dailyChart\"></canvas>\r\n                                </div>\r\n                            </v-card-text>\r\n                        </v-card>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" md=\"4\">\r\n                        <v-card>\r\n                            <v-card-title class=\"text-center\">Time Spent on Allies (Weekly)</v-card-title>\r\n                            <v-card-text>\r\n                                <div class=\"chart-container\">\r\n                                    <canvas ref=\"weeklyChart\"></c